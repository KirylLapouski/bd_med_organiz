<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.MedicalFacilityEntity" table="medical_facility" schema="medicine">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(200)" length="200"/>
        </property>
    </class>
</hibernate-mapping>